  <!-- Gazebo plugin for center RGB camera -->
  <ros2_control name="GazeboSystem" type="system">
    <hardware>
      <plugin>deepdrive_control/DeepdriveSystemHardware</plugin>
      <param name="pin_left_enable">32</param>
      <param name="pin_left_forward">38</param>
      <param name="pin_left_backward">37</param>
      <param name="pin_right_enable">33</param>
      <param name="pin_right_forward">35</param>
      <param name="pin_right_backward">36</param>
      <param name="pwm_hz">50</param>
    </hardware>
    <joint name="wheel_back_right_joint">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="wheel_front_left_joint">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="wheel_back_left_joint">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="wheel_front_right_joint">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
  </ros2_control>
