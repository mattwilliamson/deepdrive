  <!-- base_link' to 'laser -->
  <!-- <node pkg="tf2_ros" exec="static_transform_publisher" name="base_link_to_laser" args="0 0 0 3.14159 0 0 base_link laser" output="screen" /> -->


  <xacro:include filename="$(find depthai_descriptions)/urdf/include/depthai_macro.urdf.xacro"/>

  <!-- <link name="$(arg parent_frame)"/> -->
  <!-- Leave as OAK-D instead of OAK-D-LITE so the IMU is used -->
  <xacro:depthai_camera camera_name="camera" parent="base_link" camera_model="OAK-D" base_frame="camera_frame" cam_pos_x="0.08" cam_pos_y="0" cam_pos_z=".05" cam_roll="0" cam_pitch="0" cam_yaw="0"/>
</robot>
